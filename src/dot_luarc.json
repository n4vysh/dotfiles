{
  "format": {
    "enable": false
  },
  "hint": {
    "enable": true,
    "setType": true
  },
  "completion": {
    "callSnippet": "Replace"
  },
  "workspace": {
    "checkThirdParty": false
  },
  "telemetry": {
    "enable": false
  },
  "diagnostics": {
    "disable": ["lowercase-global"],
    "globals": ["Status", "Linemode"]
  }
}
