      "args": ["mcp-server-git@2025.1.14"]
    },
    "fetch": {
      "disabled": true,
      "disabled_tools": [],
      "command": "uvx",
      "args": ["mcp-server-fetch@2025.4.7"]
    },
    "playwright": {
      "disabled": true,
      "disabled_tools": [],
